import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner jee = new Scanner(System.in);
        int J = jee.nextInt();
        int A = J / 10;
        int B = J % 10;
        int C = A + B;
        System.out.println("Bird said:"+J);
        System.out.println("Alice must go in path-"+C);
    }
}
